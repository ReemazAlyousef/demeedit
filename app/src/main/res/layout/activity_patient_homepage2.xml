<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PatientHomepageActivity2">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_edit_homepage2"
        android:layout_width="190dp"
        android:layout_height="190dp"

        android:background="@drawable/round_corner_button2"
      android:backgroundTint="@color/crystalBlue_40"
        android:drawableRight="@drawable/braini"
        android:fontFamily="@font/donglebold"
        android:text="Play a GAME !"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.981"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="423dp" />

    <ImageView
        android:id="@+id/imageView_homepage"
        android:layout_width="316dp"
        android:layout_height="265dp"
        app:srcCompat="@drawable/rectangle"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="129dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_edit_homepage"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:background="@drawable/round_corner_button2"
        android:drawableRight="@drawable/heart1"
        android:fontFamily="@font/donglebold"
        android:text="Measure your Heart rate"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="423dp" />

    <ImageView
        android:id="@+id/imageView_home_page_icon"
        android:layout_width="90dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/imageprofile"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="-12dp"
        tools:ignore="MissingConstraints"
       />

    <TextView
        android:id="@+id/textView_homepage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello"
        android:textColor="@color/material_dynamic_neutral0"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="5dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_homepage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Naseer Alharbi"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="45dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_list_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My day:"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/design_default_color_on_primary"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="142dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>