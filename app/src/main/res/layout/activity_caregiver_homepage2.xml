<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CaregiverHomepageActivity2">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_edit_homepage2_cg"
        android:layout_width="190dp"
        android:layout_height="190dp"

        android:layout_marginTop="28dp"
        android:background="@drawable/round_corner_button2"
        android:backgroundTint="@color/crystalBlue_40"
        android:drawableRight="@drawable/locationicon"
        android:fontFamily="@font/donglebold"
        android:text="Find your Patient"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.981"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_homepage"
        />

    <ImageView
        android:id="@+id/imageView_homepage"
        android:layout_width="316dp"
        android:layout_height="265dp"
        app:srcCompat="@drawable/rectangle"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="129dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_edit_homepage_cg"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/round_corner_button2"
        android:drawableRight="@drawable/heart1"
        android:fontFamily="@font/donglebold"
        android:text="Heart rate"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_homepage"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView_home_page_icon"
        android:layout_width="90dp"
        android:layout_height="150dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.931"
        app:layout_constraintStart_toEndOf="@+id/textView_homepage2"
        app:srcCompat="@drawable/imageprofile"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="-12dp" />

    <TextView
        android:id="@+id/textView_homepage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello"
        android:textColor="@color/material_dynamic_neutral0"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="5dp" />

    <TextView
        android:id="@+id/textView_homepage_cg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Basil Alharthi"
        android:textColor="@color/material_dynamic_neutral_variant0"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView_home_page_icon"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_homepage2"
        />

    <TextView
        android:id="@+id/textView_list_home_cg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tasks:"
        android:textColor="@color/design_default_color_on_primary"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView_cg_homepage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nasser's"
        android:textColor="@color/design_default_color_on_primary"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView_patient_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nasser"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="479dp"
         />

</androidx.constraintlayout.widget.ConstraintLayout>